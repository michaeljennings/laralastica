{
    "name": "amrsoliman/laralastica",
    "description": "A laravel elastica package.",
    "keywords": ["elastica", "laravel", "elasticsearch"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Jennings",
            "email": "michael.jennings91@gmail.com"
        },
        {
            "name": "Amr Soliman",
            "email": "asoliman@engineer.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ruflin/elastica": "2.3.*",
        "illuminate/database": ">=5.0",
        "illuminate/events": ">=5.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Michaeljennings\\Laralastica\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Michaeljennings\\Laralastica\\Tests\\": "tests/"
      }
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 5"]
    },
    "minimum-stability": "stable"
}
